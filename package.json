{
  "name": "temp-next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:backend": "cd backend && GEMINI_API_KEY=${GEMINI_API_KEY} ../venv/bin/python -m uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:backend\"",
    "install:backend": "./venv/bin/pip install -r backend/requirements.txt",
    "build": "next build",
    "build:static": "BUILD_STATIC=true next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write ."
  },
  "dependencies": {
    "next": "^15.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-rough-notation": "^1.0.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
